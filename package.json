{
  "name": "super-scan",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "pnpm prisma:generate",
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "lint": "next lint",
    "prisma:generate": "prisma generate --no-engine",
    "prisma:db:push": "dotenvx run -f .env.local -- prisma db push",
    "prisma:studio": "dotenvx run -f .env.local -- prisma studio",
    "op-indexer": "dotenvx run -f .env.local -- pnpm dlx tsx op-indexer/index.ts",
    "super-indexer": "touch prisma/supersim.db && rm prisma/supersim.db && DATABASE_URL=file:supersim.db prisma db push && dotenvx run -f .env.local.op-chain-a -- pnpm dlx tsx op-indexer/index.ts --l1-from-block 0 -f 0 & dotenvx run -f .env.local.op-chain-b -- pnpm dlx tsx op-indexer/index.ts --l1-from-block 0 -f 0",
    "build-super-scan": "cp .env.local.op-chain-a .env.production.local && NEXT_PUBLIC_SUPERSIM=true next build && rm .env.production.local",
    "start-super-scan": "dotenvx run -f .env.local.op-chain-a -- next start -p 3001 & dotenvx run -f .env.local.op-chain-b -- next start -p 3002",
    "dev-super-scan-chain-a": "dotenvx run -f .env.local.op-chain-a -- next dev --turbopack -p 3001",
    "dev-super-scan-chain-b": "dotenvx run -f .env.local.op-chain-b -- next dev --turbopack -p 3002",
    "start-super-scan-chain-a": "dotenvx run -f .env.local.op-chain-a -- next start -p 3001",
    "start-super-scan-chain-b": "dotenvx run -f .env.local.op-chain-b -- next start -p 3002"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  },
  "dependencies": {
    "@date-fns/utc": "^2.1.0",
    "@ethereum-sourcify/bytecode-utils": "^1.3.4",
    "@ethereum-sourcify/lib-sourcify": "^1.13.0",
    "@hookform/resolvers": "^4.1.3",
    "@neondatabase/serverless": "^1.0.0",
    "@prisma/adapter-neon": "^6.6.0",
    "@prisma/client": "^6.6.0",
    "@radix-ui/react-accordion": "^1.2.4",
    "@radix-ui/react-avatar": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.7",
    "@radix-ui/react-dropdown-menu": "^2.1.7",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.3",
    "@radix-ui/react-popover": "^1.1.7",
    "@radix-ui/react-progress": "^1.1.3",
    "@radix-ui/react-select": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.3",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.1.4",
    "@radix-ui/react-tabs": "^1.1.4",
    "@radix-ui/react-tooltip": "^1.2.0",
    "@reown/appkit": "^1.7.2",
    "@reown/appkit-adapter-wagmi": "^1.7.2",
    "@rjsf/core": "^5.24.8",
    "@rjsf/utils": "^5.24.8",
    "@rjsf/validator-ajv8": "^5.24.8",
    "@tanstack/react-query": "^5.72.2",
    "abitype": "^1.0.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.483.0",
    "next": "^15.3.0",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.55.0",
    "react-json-tree": "^0.20.0",
    "react-syntax-highlighter": "^15.6.1",
    "react-syntax-highlighter-virtualized-renderer": "^1.1.0",
    "recharts": "^2.15.2",
    "solc-0.8.24": "npm:solc@0.8.24",
    "solc-0.8.25": "npm:solc@0.8.25",
    "solc-0.8.26": "npm:solc@0.8.26",
    "solc-0.8.27": "npm:solc@0.8.27",
    "solc-0.8.28": "npm:solc@0.8.28",
    "solc-0.8.29": "npm:solc@0.8.29",
    "tailwind-merge": "^3.2.0",
    "viem": "^2.26.2",
    "wagmi": "^2.14.16",
    "ws": "^8.18.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
